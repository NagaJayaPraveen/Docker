node('master') 
     {
    stage('continuous download')
           {
              git branch: 'main', url: 'https://github.com/NagaJayaPraveen/Docker.git'/root/DockerFiles

           }

    stage('continuous build')
           {
sh '''
 
mv image5 ~/DockerFiles
cd DockerFiles &&  docker build -t image5:1.0 .'''
           }
     }
