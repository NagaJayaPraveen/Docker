node('master') 
     {
    stage('continuous download')
           {
              git branch: 'main', url: 'https://github.com/NagaJayaPraveen/Docker.git'
           }

    stage('continuous build')
           {
sh '''mv image5 ~/
cd /var/lib/jenkins/
mv image5 ~/
docker build -t image5:1.0 .'''
           }
     }
